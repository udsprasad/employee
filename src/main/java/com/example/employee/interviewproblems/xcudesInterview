array =[1,2,34,5,6,7]

class MinAndMax{
public static void main(String[] args){
int[] array =[1,2,34,5,6,7];
int len =array.length;
int max=0,min=Integer.MAX_VALUE;
for(int i=0;i<len;i++){
if(i>max){
max=i;
}
else if(i<min){
min=i;
}
}
Systemout.println(min+" "+max);
}
}

