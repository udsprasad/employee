put("first","1");
put("second","2")
}};
map.forEach(System.out::println) or forEach((k,v)-> sysout(k+" "+v));
map.entrySet().stream().foreach(System.out::println);

User entity 20 attributes VO first,last,email

List<User> list

List<UserVO> list1;

for(User u: list){
UserVO userVO = UserVO.Builder().firstName(u.firstName).lastName(u.lastName).email(u.email).build();
list1.add(userVO);
}

